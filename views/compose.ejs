<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="POST">
	<div class="form-group">
		<label for="composeTitle">Title</label>
		<input class="form-control" type="text" name="composeTitle">
	</div>
	<div class="form-group">
		<label for="composeBody">Post</label>
		<textarea class="form-control" rows="3" name="composeBody"></textarea>
	</div>
		<button class="btn btn-primary" value="submit" name="button">Publish</button>

</form>

<%- include("partials/footer") -%>